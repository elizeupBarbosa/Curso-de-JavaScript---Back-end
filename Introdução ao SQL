SELECT DESC_PROD, UNID_PROD, COD_PROD, VAL_UNIT FROM PRODUTO;

SELECT NOME_CLI, CIDADE, UF FROM CLIENTE WHERE UF != 'RJ';

SELECT NO_PED, CD_PROD, QTD_PED FROM ITEM_PEDIDO
    WHERE QTD_PED = 500;
    
SELECT DESC_PROD FROM PRODUTO
    WHERE TRIM(UNID_PROD) = 'KG'AND VAL_UNIT > 2;
    
SELECT NOME_CLI, CIDADE, CEP FROM CLIENTE
    WHERE CIDADE = 'SP' OR CEP BETWEEN 20000005 AND 20000010;
    
SELECT COD_PROD, DESC_PROD FROM PRODUTO
    WHERE VAL_UNIT BETWEEN 0.10 AND 3;
    
SELECT DESC_PROD, UNID_PROD FROM PRODUTO
    WHERE UNID_PROD LIKE ('K%');
    
SELECT NOME_VEND, FAIXA_COMISS FROM VENDEDOR
    WHERE FAIXA_COMISS IN ('A', 'B');
    
SELECT NOME_CLI, ENDERECO FROM CLIENTE
    WHERE ENDERECO IS NOT NULL;
    
SELECT NOME_VEND, SAL_FIXO FROM VENDEDOR
    ORDER BY NOME_VEND;
    
SELECT NOME_VEND, SAL_FIXO, ((SAL_FIXO * 1.75) + 120) AS NOVO_SAL
    FROM VENDEDOR
    WHERE FAIXA_COMISS = 'C'
    ORDER BY NOME_VEND;
    
SELECT MIN(SAL_FIXO), MAX(SAL_FIXO) FROM VENDEDOR;
SELECT AVG(SAL_FIXO) FROM VENDEDOR;
SELECT COUNT(*) FROM VENDEDOR
    WHERE SAL_FIXO = 900;
    
SELECT DISTINCT CIDADE FROM CLIENTE;

SELECT NO_PED, COUNT(*) AS TOTAL_ITENS FROM ITEM_PEDIDO
    GROUP BY NO_PED
    HAVING COUNT(*) > 1;
